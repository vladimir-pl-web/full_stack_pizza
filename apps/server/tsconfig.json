{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "experimentalDecorators": true,
    "noEmit": true,
    //"allowImportingTsExtensions": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "paths": {
      "@items/*": ["prisma/seeder/seed/items/*"]
